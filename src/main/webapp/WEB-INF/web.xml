<?xml version="1.0" encoding="UTF-8"?>
<web-app 
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
    version="2.5">
  <display-name>Logging &amp; Echo &amp; Form</display-name>
  
  <servlet>
    <servlet-name>EchoFormServlet</servlet-name>
    <servlet-class>org.eclipse.jetty.examples.logging.EchoFormServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>EchoFormServlet</servlet-name>
    <url-pattern>/echoform</url-pattern>
  </servlet-mapping>
</web-app>
